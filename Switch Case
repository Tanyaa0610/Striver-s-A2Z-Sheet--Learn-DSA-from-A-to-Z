class Solution {
public:
    void whichWeekDay(int day) {
        switch(day){
            case 1:
            cout<<"Monday";
            case 2:
            cout<<"Tuesday";
            case 3:
            cout<<"Wednesday";
            case 4:
            cout<<"Thursday";
            case 5:
            cout<<"Friday";
            case 6:
            cout<<"Saturday"; 
            default:
            cout<<"Invaild";
        }

    }
};
